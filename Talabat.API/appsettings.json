{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=.; Database=TalabatDB; Trusted_Connection=true; MultipleActiveResultSets=true",
    "IdentityConnection": "Server=.; Database=TalabatIdentityDB; Trusted_Connection=true; MultipleActiveResultSets=true",
    "RedisConnection": "localhost"
  },
  "BaseApiUrl": "https://localhost:5001/",
  "JWT": {
    "Key": "StringAuthenticationKey",
    "ValidIssuer": "https://localhost:5001/",
    "ValidAudience": "MySecuredApiUsers",
    "DurationInDays": 30
  }
}
